			</aux>
			<aux name="TROP_Sum_outflows_GRASS_Dead_biomass,_litter_and_soil_organic_matter_SOM_GtBiomass/yr">
				<units>GtBiomass/yr</units>
				<doc></doc>
					<eqn>"TROP_DeadB_SOM_being_lost_due_to_deforestation_GtBiomass/yr"
	+ "TROP_DeadB_SOM_being_lost_due_to_energy_harvesting_GtBiomass/yr"
	+ TROP_runoff
	+ "TROP_soil_degradation_from_clear_cutting_GtBiomass/yr"
