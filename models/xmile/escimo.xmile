					<eqn>( Tundra_potential_area_Mkm2 - TUNDRA_deforested_Mkm2 ) * TUNDRA_living_biomass_densitiy_tBiomass_pr_km2 / "UNIT_conversion_GtBiomass/yr_to_Mkm2/yr"					</eqn>
			</aux>
			<aux name="TUNDRA_regrowing_after_being_burnt_Mkm2/yr">
