					<eqn>TUNDRA_land_taken_out_of_use_Mkm2 * TUNDRA_living_biomass_densitiy_tBiomass_pr_km2 / "UNIT_conversion_GtBiomass/yr_to_Mkm2/yr"					</eqn>
			</aux>
			<aux name="TUNDRA_land_taken_out_of_use_Mkm2">
				<units>Mkm2</units>
				<doc></doc>
					<eqn>TUNDRA_deforested_Mkm2 + TUNDRA_area_burnt_Mkm2 + TUNDRA_area_harvested_Mkm2					</eqn>
			</aux>
			<aux name="TUNDRA_living_biomass_densitiy_tBiomass_pr_km2">
				<units>tB/km2</units>
				<doc></doc>
					<eqn>TUNDRA_living_biomass_densitiy_in_1850_tBiomass_pr_km2 * Effect_of_CO2_on_new_biomass_growth * Effect_of_temperature_on_new_biomass_growth_dimensionless					</eqn>
			</aux>
			<aux name="TUNDRA_Living_biomass_rotting_GtBiomass/yr">
				<units>GtBiomass/yr</units>
