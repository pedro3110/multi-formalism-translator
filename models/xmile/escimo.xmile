				<doc></doc>
					<eqn>Tundra_potential_area_Mkm2 - TUNDRA_area_burnt_Mkm2 - TUNDRA_deforested_Mkm2 - TUNDRA_area_harvested_Mkm2					</eqn>
			</aux>
			<aux name="UNIT_conversion_for_CH4_from_CO2e_to_C">
				<units>GtC/GtCO2e</units>
				<doc></doc>
					<eqn>1/(16/12 * Global_Warming_Potential_CH4)					</eqn>
			</aux>
			<aux name="UNIT_conversion_for_CO2_from_CO2e_to_C">
				<units>GtC/GtCO2e</units>
				<doc></doc>
					<eqn>12/44					</eqn>
			</aux>
			<aux name="UNIT_conversion_from_MtCH4_to_GtC">
				<units>GtC/MtCH4</units>
				<doc></doc>
					<eqn>1 /( 1000 / 12 * 16)					</eqn>
			</aux>
			<aux name="UNIT_conversion_GtCO2e_to_GtC">
				<units>GtC/GtCO2e</units>
				<doc></doc>
					<eqn>12/44					</eqn>
			</aux>
			<aux name="UNIT_conversion_mm_to_m">
				<units>m/mm</units>
				<doc></doc>
					<eqn>1/1000					</eqn>
			</aux>
			<aux name="UNIT_conversion_W/m2_earth_to_ZJ/yr">
				<units>m2*ZJ/(yr*W)</units>
				<doc></doc>
					<eqn>(Seconds_per_yr * Area_of_earth_m2) * "UNIT_conversion_W_to_ZJ/s" / Zetta					</eqn>
			</aux>
