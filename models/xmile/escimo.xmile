			<aux name="BARREN_land_white_Mkm2">
				<units>Mkm2</units>
				<doc></doc>
					<eqn>IF_THEN_ELSE(Switch_to_run_experiment_12e_white_surfaces_0_off_1_on = 1, EXP_12e_white_surfaces_ease_in, 0)					</eqn>
			</aux>
			<aux name="BB_radiation_at_surface_temp_ZJ/yr">
				<units>ZJ/yr</units>
				<doc></doc>
					<eqn>Emissivity_surface * Stephan_Boltzmann_constant * Temp_surface_average_K^4 * "UNIT_conversion_W/m2_earth_to_ZJ/yr"					</eqn>
			</aux>
			<aux name="BB_radiation_at_Temp_in_atm_ZJ/yr">
				<units>ZJ/yr</units>
				<doc></doc>
					<eqn>Emissivity_atm * Stephan_Boltzmann_constant * Temp_atm_average_K^4 * "UNIT_conversion_W/m2_earth_to_ZJ/yr"					</eqn>
			</aux>
			<aux name="Biological_removal_of_C_from_WSW_GtC_per_yr">
				<units>GtC/yr</units>
				<doc></doc>
					<eqn>SMOOTH(Net_marine_primary_production_NMPP_GtC_pr_yr , Time_to_let_shells_form_and_sink_to_sediment_yr)					</eqn>
			</aux>
			<aux name="Blocked_by_CH4">
				<units>fraction</units>
				<doc></doc>
					<eqn>Fraction_blocked_by_CH4_spectrum					</eqn>
			</aux>
			<aux name="Blocked_by_CO2">
