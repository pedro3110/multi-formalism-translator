			</aux>
			<aux name="TUNDRA_DeadB_and_SOM_tB_per_km2">
				<units>tB/km2</units>
				<doc></doc>
					<eqn>TUNDRA_DeadB_and_SOM_densitiy_in_1850_tBiomass_pr_km2 * Effect_of_CO2_on_new_biomass_growth * Effect_of_temperature_on_new_biomass_growth_dimensionless					</eqn>
			</aux>
			<aux name="TUNDRA_DeadB_SOM_being_lost_due_to_deforestation_GtBiomass/yr">
				<units>GtBiomass/yr</units>
				<doc>/ 1000 to convert M (illion) to G (iga)</doc>
					<eqn>"TUNDRA_being_deforested_Mkm2/yr" * TUNDRA_fraction_of_DeadB_and_SOM_being_destroyed_by_deforesting * TUNDRA_DeadB_and_SOM_tB_per_km2
	 / "UNIT_conversion_GtBiomass/yr_to_Mkm2/yr"					</eqn>
			</aux>
			<aux name="TUNDRA_DeadB_SOM_being_lost_due_to_energy_harvesting_GtBiomass/yr">
				<units>GtBiomass/yr</units>
				<doc></doc>
					<eqn>"TUNDRA_being_harvested_Mkm2/yr" * TUNDRA_fraction_of_DeadB_and_SOM_being_destroyed_by_energy_harvesting * TUNDRA_DeadB_and_SOM_tB_per_km2
	 / "UNIT_conversion_GtBiomass/yr_to_Mkm2/yr"					</eqn>
			</aux>
			<aux name="TUNDRA_for_construction_use_GtBiomass/yr">
				<units>GtBiomass/yr</units>
				<doc></doc>
					<eqn>"Use_of_TUNDRA_biomass_for_construction_GtBiomass/yr"					</eqn>
			</aux>
			<aux name="TUNDRA_historical_deforestation_pct/yr">
				<units>1/yr</units>
				<doc></doc>
					<eqn>("TUNDRA_Ref_historical_deforestation_pct/yr" / 100)					</eqn>
			</aux>
			<aux name="TUNDRA_land_taken_out_of_use_GtBiomass">
