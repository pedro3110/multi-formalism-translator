					<eqn>"TUNDRA_burning_Mkm2/yr" * TUNDRA_DeadB_and_SOM_tB_per_km2 / "UNIT_conversion_GtBiomass/yr_to_Mkm2/yr" * TUNDRA_fraction_of_DeadB_and_SOM_destroyed_by_natural_fires					</eqn>
			</aux>
			<aux name="TUNDRA_Sum_outflows_GRASS_Dead_biomass,_litter_and_soil_organic_matter_SOM_GtBiomass/yr">
				<units>GtBiomass/yr</units>
				<doc></doc>
					<eqn>"TUNDRA_DeadB_SOM_being_lost_due_to_deforestation_GtBiomass/yr"
	+ "TUNDRA_DeadB_SOM_being_lost_due_to_energy_harvesting_GtBiomass/yr"
